{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "redirects": [
    {
      "source": "/q/:code",
      "destination": "https://anwwjowwrxdygqyhhckr.supabase.co/functions/v1/qr-redirect/:code",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/:path((?!q).*)*",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_REACT_COMPILER_ENABLED": "true",
    "VITE_OPTIMIZATION_MODE": "production"
  }
}